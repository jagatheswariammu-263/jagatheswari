data = pd.concat([test_data, text_data, numerical_data], axis=1)\n"],"metadata":{"id":"JLhijQmDq7Zo"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["# Define Neural Collaborative Filtering (NCF) model\n","model = Sequential()\n","model.add(Embedding(input_dim=num_users, output_dim=64, input_length=1))\n","model.add(Embedding(input_dim=num_items, output_dim=64, input_length=1))\n","model.add(Dropout(0.2))\n","model.add(Dense(64, activation='relu'))\n","model.add(Dropout(0.2))\n","model.add(Dense(1, activation='sigmoid'))\n","model.compile(loss='binary_crossentropy', optimizer=Adam(lr=0.001))\n"],"metadata":{"id":"oeF8XEn6rFOs"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["# Train NCF model\n","model.fit([train_data['user_id'], train_data['item_id']], train_data['rating'], epochs=10, batch_size=256)"],"metadata":{"id":"segr0MjDrRc8"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["\n","# Evaluate NCF model on test data\n","test_pred = model.predict([test_data['user_id'], test_data['item_id']])\n","test_pred_binary = (test_pred > 0.5).astype('int32')\n","accuracy = accuracy_score(test_data['rating'], test_pred_binary)\n","f1 = f1_score(test_data['rating'], test_pred_binary, average='macro')\n","print(\"Accuracy:\", accuracy)\n","print(\"F1-score:\", f1)"],"metadata":{"id":"V4f329o0rgke"},"execution_count":null,"outputs":[]}]}